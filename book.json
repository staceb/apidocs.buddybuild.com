{
  "description": "buddybuild API Documentation",
  "author": "The buddybuild team",
  "language": "en",
  "structure": {
    "readme": "index.adoc"
  },
  "styles": {
    "website": "_css/website.css"
  },
  "plugins": [
    "-sharing",
    "search@2.2.1",
    "explicit-url@0.0.3",
    "theme-api@git+https://github.com/BuddyBuild/theme-api.git",
    "buddybuild@git+https://github.com/buddybuild-public/gitbook-plugin-buddybuild.git",
    "edit-link@https://github.com/buddybuild-public/gitbook-plugin-edit-link.git"
  ],
  "pluginsConfig": {
    "theme-api": {
      "languages": [
        {
          "lang": "js",
          "name": "JavaScript",
          "default": true
        },
        {
          "lang": "go",
          "name": "Go"
        }
      ],
      "split": false
    },
    "edit-link": {
      "base": "https://github.com/buddybuild-public/apidocs.buddybuild.com/edit/master",
      "label": "",
      "aria": "Edit on GitHub",
      "position": "right",
      "cssclass": "bbedit"
    }
  }
}
